@model IEnumerable<UYIOSA.Models.AdminDetails>

@{
    ViewBag.Title = "ManageAdmin";
    Layout = "~/Views/Shared/_AdminPageLayout.cshtml";
}

<h2>ManageAdmin</h2>

<p>
    @Html.ActionLink("Add New Admin", "RegisterAdmin")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MatNo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FirstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LastName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Status)
        </th>
       
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MatNo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LastName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Status)
        </td>
        
        <td>
            @Html.ActionLink("Remove", "RemoveAdmin", new { id=item.Id }) 
           
        </td>
    </tr>
}

</table>
